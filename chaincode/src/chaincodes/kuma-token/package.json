{
    "name": "kuma-token",
    "version": "1.0.0",
    "description": "This chaincode implements to kuma-token and it's wallet contracts.",
    "engines": {
        "node": ">=8.4.0",
        "npm": ">=5.3.0"
    },
    "scripts": {
        "start": "CORE_CHAINCODE_LOGGING_SHIM=DEBUG node chaincode.js"
    },
    "engine-strict": true,
    "engineStrict": true,
    "license": "MIT",
    "dependencies": {
        "@kunstmaan/hyperledger-fabric-node-chaincode-utils": "^0.2.0",
        "fabric-shim": "git+https://github.com/daanporon/fabric-shim.git"
    },
    "hf-language": "node",
    "hf-dev-channel": "defaultchannel",
    "hf-network": [
        {
            "channelId": "defaultchannel",
            "instantiateArgs": [],
            "endorsementPolicy": "OR('Org1-MSP.member')",
            "peers": [
                "peer.org1.example.be"
            ],
            "orderer": {
                "host": "orderer.org1.example.be",
                "port": 7050
            }
        }
    ]
}
